{
  "family": "zalo-phake-chat-be-task",
  "containerDefinitions": [
    {
      "name": "zalo-phake-chat-be",
      "image": "python:3.9-alpine",
      "cpu": 128,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "command": [
        "uvicorn",
        "app.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "5000"
      ],
      "environment": [
        {
          "name": "PATH_PREFIX",
          "value": "chat-management"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/zalo-phake-be-task",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::249720777545:role/Execution-ECS",
  "taskRoleArn": "arn:aws:iam::249720777545:role/Execution-ECS"
}
